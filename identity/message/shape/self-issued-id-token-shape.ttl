@prefix : <http://example.org/ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:SelfIssuedIDToken a schema:Thing ;
    schema:name "Self-Issued ID Token" ;
    schema:description "A self-issued identity token" ;
    :sub [ a schema:Text ] ;
    :iss [ a schema:Text ] ;
    :iat [ a schema:Number ] ;
    :exp [ a schema:Number ] ;
    :jti [ a schema:Text ] ;
    :did_document [ a schema:Thing ;
        schema:context [ a schema:URL ] ;
        schema:id [ a schema:Text ] ;
        :service [
            a schema:ItemList ;
            schema:itemListElement [ a schema:Thing ;
                schema:id [ a schema:Text ] ;
                schema:type [ a schema:Text ] ;
                :serviceEndpoint [ a schema:URL ]
            ]
        ]
    ] ;
    :civ [ a schema:ItemList ; schema:itemListElement [ a schema:Text ] ] ;
    :vcs [ a schema:ItemList ; schema:itemListElement [ a schema:Text ] ] .
