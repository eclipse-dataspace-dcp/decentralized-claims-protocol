@prefix : <http://example.org/ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:OAuth2ClientCredentialGrant a schema:Thing ;
    schema:name "OAuth 2 Client Credential Grant" ;
    schema:description "An OAuth 2 client credential grant" ;
    :grant_type [ a schema:Text ; schema:value "client_credentials" ] ;
    :client_id [ a schema:Text ] ;
    :client_secret [ a schema:Text ] ;
    :scope [ a schema:Text ] .
