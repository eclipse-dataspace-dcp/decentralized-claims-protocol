{
  "@context": [
    "https://w3id.org/dspace-dcp/v0.8",
    "https://www.w3.org/2018/credentials/v1",
    "https://www.w3.org/2018/credentials/examples/v1"
  ],
  "type": "CredentialObject",
  "credentialType": [
    "VerifiableCredential",
    "CompanyCredential"
  ],
  "format": "ldp_vc",
  "offerReason": "reissue",
  "bindingMethods": [
    "did:web"
  ],
  "cryptography": [
    "JsonWebSignature202", "eddsa-rdfc-2022", "eddsa-jcs-2022", "..."
  ],
  "issuancePolicy": {
    "permission": {
      "action": "use",
      "constraint": {
        "and": {
          "leftOperand": "CredentialPrereq",
          "operator": "eq",
          "rightOperand": "active"
        }
      }
    }
  }
}