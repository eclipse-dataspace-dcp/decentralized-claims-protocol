{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Identity and Trust Protocol Context",
    "type": "object",
    "properties": {
        "@context": {
            "type": "string",
            "format": "uri",
            "description": "JSON-LD context URI"
        },
        "version": {
            "type": "string",
            "description": "Semantic versioning MAJOR.MINOR number"
        }
    },
    "required": ["@context", "version"]
}
